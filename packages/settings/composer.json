{
    "name": "vendor/settings",
    "description": "Settings management package for Laravel - Menu, Banner, Slide, and System Configuration",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "package",
        "settings",
        "configuration"
    ],
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Settings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Settings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\Settings\\SettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

