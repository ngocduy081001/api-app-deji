{
    "name": "vendor/customer",
    "description": "Customer management package for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "package",
        "customer",
        "management"
    ],
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Customer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Customer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\Customer\\CustomerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

