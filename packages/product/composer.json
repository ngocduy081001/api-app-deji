{
    "name": "vendor/product",
    "description": "A Laravel package",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "package"
    ],
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Product\\": "src/",
            "Vendor\\Product\\Database\\Factories\\": "database/factories/",
            "Vendor\\Product\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Product\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\Product\\ProductServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
